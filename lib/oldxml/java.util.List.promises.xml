<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE package SYSTEM "promises.dtd">

<package name="java.util">
    <class name="List">

  <promise keyword="promise"
    contents="&quot;transparent&quot; for *(**)"/>


        <method name="get" params="int">
          <promise keyword="borrowed" contents="this"/>
          <promise keyword="reads" contents="Instance"/>
        </method>
    
        <method name="addAll" params="java.util.Collection">
          <promise keyword="borrowed" contents="this"/>
          <promise keyword="writes" contents="Instance"/>
          <promise keyword="reads" contents="arg0.Instance"/>
        </method>

        <method name="add" params="*">
          <promise keyword="borrowed" contents="this"/>
          <promise keyword="writes" contents="Instance"/>
        </method>

        <method name="clear" params="">
          <promise keyword="borrowed" contents="this"/>
          <promise keyword="writes" contents="Instance"/>
        </method>

        <method name="remove" params="java.lang.Object">
          <promise keyword="borrowed" contents="this"/>
          <promise keyword="writes" contents="Instance"/>
        </method>

        <method name="size" params="">
          <promise keyword="borrowed" contents="this"/>
          <promise keyword="reads" contents="Instance"/>
        </method>

        <method name="iterator" params="">
          <promise keyword="reads" contents="Instance"/>
        </method>

        <method name="listIterator" params="int">
          <promise keyword="reads" contents="Instance"/>
        </method>
    </class>  
</package>
