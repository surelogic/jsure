<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE package SYSTEM "promises.dtd">

<package name="java.awt">
    <class name="Button">
    <promise keyword="colorImport" contents="java.awt.*;"/>
    
    <promise keyword="mapRegion"
      contents="Instance into java.awt.event.ActionListener#GUIstuff"/>

    <promise keyword="promise"
	    contents="&quot;transparent&quot; for add*Listener(**) | remove*Listener(**)"/>
	  <promise keyword="promise"
	    contents="&quot;writes java.awt.event.ActionListener.GUIinternal&quot; for add*Listener(**) | remove*Listener(**)"/>

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for get*(**) | new(**)"/>
  <promise keyword="promise"
    contents="&quot;reads java.awt.event.ActionListener.GUIinternal&quot; for new(**)"/>
  <promise keyword="promise"
    contents="&quot;reads Instance&quot; for get*(**)"/>

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for add*(**) &amp; !(add*Listener(**))"/>
  <promise keyword="promise"
    contents="&quot;writes Instance&quot; for add*(**) &amp; !(add*Listener(**))"/>  

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for process*(**)"/>
  <promise keyword="promise"
    contents="&quot;reads java.awt.event.ActionListener.GUIinternal&quot; for process*(**)"/>  
  <promise keyword="promise"
    contents="&quot;writes Instance&quot; for process*(**)"/>  

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for set*(**)"/>
  <promise keyword="promise"
    contents="&quot;writes Instance&quot; for set*(**)"/>

    </class>  
</package>
