        <promise keyword="promise"
	  contents="&quot;colorConstraint GUI;&quot; for put*(**) | invalidate(**)"/>
	  
	  <promise keyword="promise"
	  	  contents="&quot;writes java.awt.event.ActionListener.GUIinternal&quot; for add(**) | remove(**)"/>
	  <promise keyword="promise"
	  	  contents="&quot;writes java.awt.event.ActionListener.GUIinternal&quot; for set*(**) | create*(**)"/>
    <promise keyword="promise"
	  	  contents="&quot;reads java.awt.event.ActionListener.GUIinternal&quot; for get*(**) | is*(**)"/>

        <method name="paint" params="java.awt.Graphics">
