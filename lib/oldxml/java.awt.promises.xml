<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE package SYSTEM "promises.dtd">

<package name="java.awt">
  <promise keyword="color" contents="AWT, Compute, NotYetVisible"/>
  <promise keyword="incompatibleColors" contents="AWT, Compute"/>
  <promise keyword="colorRename" contents="GUI for (AWT | NotYetVisible);"/>
  <promise keyword="colorRename" contents="GUIorCompute for (GUI | Compute);"/>
  
  
  <promise keyword="promise"
  	contents="&quot;@colorImport java.awt.*;&quot; for *"/>
 
  <promise keyword="promise"
  	contents="&quot;transparent&quot; for *:add*Listener(**) | remove*Listener(**)"/>
  <promise keyword="promise"
    contents="&quot;writes java.awt.event.ActionListener.GUIinternal&quot; for *:add*Listener(**) | remove*Listener(**)"/>

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for get*(**) &amp; !(getSize(**))"/>

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for add*(**) &amp; !(add*Listener(**))"/>

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for new(**)"/>

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for clone()"/>
  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for process*(**)"/>

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for set*(**) | show*(**)"/>
    
  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for create*(**) | derive*(**)"/>    

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for updateUI()"/>
    
  <promise keyword="promise"
    contents="&quot;transparent&quot; for revalidate()"/>

  <promise keyword="promise"
    contents="&quot;transparent&quot; for repaint(**)"/>

  <promise keyword="promise"
    contents="&quot;colorConstraint AWT;&quot; for fire*(**)"/>


  <promise keyword="promise"
    contents="&quot;colorConstraint AWT;&quot; for requestFocus*(**)"/>


</package>
