<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE package SYSTEM "promises.dtd">

<package name="javax.swing">
    <class name="AbstractButton">
    <promise keyword="colorImport" contents="java.awt.*;"/>
        <promise keyword="promise" 
	  contents="&quot;transparent&quot; for add*Listener(**)"/>
        <promise keyword="promise" 
	  contents="&quot;transparent&quot; for remove*Listener(**)"/>
        <promise keyword="promise" 
	  contents="&quot;colorConstraint AWT;&quot; for getAction*(**)"/>


        <promise keyword="promise" 
	  contents="&quot;colorConstraint GUI;&quot; for *(**) &amp; !(add*Listener(**)) &amp; !(remove*Listener(**)) &amp; !(getAction*(**))"/>
    </class>
</package>

