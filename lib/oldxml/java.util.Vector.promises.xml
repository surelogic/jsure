<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE package SYSTEM "promises.dtd">

<package name="java.util">
    <class name="Vector">
        <promise keyword="promise"
	  contents="&quot;transparent&quot; for *(**)"/>

        <constructor>
            <promise keyword="borrowed" contents="this"/>
        </constructor>

        <method name="clone" params="">
          <promise keyword="borrowed" contents="this"/>
          <promise keyword="reads" contents="Instance"/>
          <promise keyword="return" contents="{@unique}"/>
        </method>

        <method name="toString" params="">
          <promise keyword="borrowed" contents="this"/>
          <promise keyword="reads" contents="Instance"/>
        </method>

        <method name="removeAllElements" params="">
          <promise keyword="borrowed" contents="this"/>
          <promise keyword="writes" contents="Instance"/>
        </method>

        <method name="addElement" params="java.lang.Object">
          <promise keyword="borrowed" contents="this"/>
          <promise keyword="writes" contents="Instance"/>
        </method>

        <method name="size" params="">
          <promise keyword="borrowed" contents="this"/>
          <promise keyword="reads" contents="Instance"/>
        </method>

        <method name="elementAt" params="int">
          <promise keyword="borrowed" contents="this"/>
          <promise keyword="reads" contents="Instance"/>
        </method>
    </class>  
</package>
