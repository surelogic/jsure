<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE package SYSTEM "promises.dtd">

<package name="java.lang">
    <class name="Class">
	  <promise keyword="abstractImmutable"/>		
      <promise keyword="promise"
	contents="&quot;transparent&quot; for *(**)"/>

        <method name="getName">
          <promise keyword="reads" contents="this.Instance"/>
          <promise keyword="starts" contents="nothing"/>
        </method>
    </class>  
</package>
