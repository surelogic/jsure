<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE package SYSTEM "promises.dtd">

<package name="java.awt">
    <class name="Window">

    <promise keyword="colorImport" contents="java.awt.*;"/>

      <promise keyword="promise"
	contents="&quot;transparent&quot; for add*Listener(**) | remove*Listener(**)"/>

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for get*(**) | is*(**)"/>

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for add*(**) &amp; !(add*Listener(**))"/>

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for process*(**) | dispose()"/>

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for set*(**)"/>

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for toFront() | toBack()"/>

  <promise keyword="promise"
    contents="&quot;colorConstraint GUI;&quot; for pack() | show() | hide()"/>

    </class>
</package>
