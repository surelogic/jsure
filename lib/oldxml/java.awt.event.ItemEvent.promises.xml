<!DOCTYPE package SYSTEM "promises.dtd">

<package name="java.awt.event">
    <class name="ItemEvent">
    <promise keyword="colorImport" contents="java.awt.*;"/>
    <promise keyword="promise"
       contents="&quot;MapInto java.awt.event.ActionListener#GUIinternal&quot; for * *"/>
        <method name="getStateChange" params="">
            <promise keyword="colorConstraint" contents="AWT;"/>
            <promise keyword="reads" contents="java.awt.event.ActionListener.GUIinternal"/>
        </method>
    </class>
</package>
