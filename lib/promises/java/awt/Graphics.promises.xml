<package name="java.awt">
  <class name="Graphics">

    <ThreadRoleImport>java.awt</ThreadRoleImport>

    <!-- Promise>@MapRegion Instance into java.awt.event.ActionListener:GUIstuff)</Promise -->

    <Promise>@ThreadRole(AWT) for get*(**)</Promise>
    <Promise>@RegionEffects(reads java.awt.event.ActionListener:GUIinternal) for get*(**)</Promise>
    
    <Promise>@ThreadRole(AWT) for set*(**)</Promise>
    <Promise>@RegionEffects(writes java.awt.event.ActionListener:GUIinternal) for set*(**)</Promise>
    
    <Promise>@ThreadRole(AWT) for draw*(**)</Promise>
    <Promise>@RegionEffects(reads Instance; writes java.awt.event.ActionListener:GUIinternal) for draw*(**)</Promise>
    
    <Promise>@ThreadRole(AWT) for fill*(**)</Promise>
    <Promise>@RegionEffects(reads Instance; writes java.awt.event.ActionListener:GUIinternal) for fill*(**)</Promise>
    
    
    <Promise>@ThreadRole(AWT) for clip*(**)</Promise>
    
    <Promise>@ThreadRole(AWT) for copy*(**)</Promise>
    
    <Promise>@ThreadRole(AWT) for create(**) | translate(**)</Promise>
    
    <method name="dispose" params="">
      <ThreadRole>AWT</ThreadRole>
      <RegionEffects>writes Instance</RegionEffects>
    </method>
    
    
  </class>
</package>
