<?xml version="1.0" encoding="UTF-8"?>

<package name="javax.swing">
	<class name="JComboBox">
		<ThreadRoleImport>java.awt</ThreadRoleImport>
		<Promise>@ThreadRoleTransparent&quot; for add*Listener(**) |
			remove*Listener(**)</Promise>

		<Promise>@ThreadRole(AWT) for remove*(**) &amp;
			!(remove*Listener(**))</Promise>
		<Promise>@ThreadRole(AWT) for process*(**)</Promise>
		<Promise>@ThreadRole(AWT) for insert*(**) | hidePopup()</Promise>

		<Promise>@ThreadRole(AWT) for updateUI()</Promise>

		<Promise>@ThreadRole(AWT) for fire*(**)</Promise>

	</class>
</package>
