<?xml version="1.0" encoding="UTF-8"?>
 
<package name="javax.swing.plaf.synth">
	<Promise>@ThreadRoleImport("java.awt") for *</Promise>

	<Promise>@ThreadRoleTransparent for add*Listener(**) |
		remove*Listener(**)</Promise>
	<Promise>@ThreadRoleTransparent for repaint(**)</Promise>
	<Promise>@ThreadRoleTransparent for revalidate()</Promise>

	<Promise>@ThreadRole(AWT) for get*(**)</Promise>
	<Promise>@ThreadRole(AWT) for add*(**) &amp; !(add*Listener(**))</Promise>
	<Promise>@ThreadRole(AWT) for new(**)</Promise>
	<Promise>@ThreadRole(AWT) for process*(**)</Promise>
	<Promise>@ThreadRole(AWT) for set*(**)</Promise>
	<Promise>@ThreadRole(AWT) for updateUI()</Promise>


	<Promise>@ThreadRole(AWT) for fire*(**)</Promise>
	<Promise>@ThreadRole(AWT) for requestFocus*(**)</Promise>
	<Promise>@ThreadRole(AWT) for paint*(**)</Promise>
</package>
