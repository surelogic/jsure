<?xml version="1.0" encoding="UTF-8"?>

<package name="javax.swing">
	<class name="AbstractButton">
		<ThreadRoleImport>java.awt</ThreadRoleImport>
		<Promise>@ThreadRoleTransparent for add*Listener(**)</Promise>
		<Promise>@ThreadRoleTransparent for remove*Listener(**)</Promise>
		<Promise>@ThreadRole("AWT") for getAction*(**)</Promise>


		<Promise>@ThreadRole("GUI") for *(**) &amp; !(add*Listener(**)) &amp;
			!(remove*Listener(**)) &amp; !(getAction*(**))</Promise>
	</class>
</package>

