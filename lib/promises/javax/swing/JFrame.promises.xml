<?xml version="1.0" encoding="UTF-8"?>

<package name="javax.swing">
	<class name="JFrame">
		<ThreadRoleImport>java.awt</ThreadRoleImport>

		<Promise>@ThreadRole("AWT") for get*(**)</Promise>
		<Promise>@ThreadRole("AWT") for set*(**)</Promise>
		<Promise>@ThreadRole("AWT") for update(*)</Promise>

		<constructor>
			<ThreadRole>AWT</ThreadRole>
		</constructor>

		<constructor params="java.awt.GraphicsConfiguration">
			<ThreadRole>AWT</ThreadRole>
		</constructor>

		<constructor params="java.lang.String">
			<ThreadRole>AWT</ThreadRole>
		</constructor>

		<constructor params="java.lang.String, java.awt.GraphicsConfiguration">
			<ThreadRole>AWT</ThreadRole>
		</constructor>

	</class>
</package>
