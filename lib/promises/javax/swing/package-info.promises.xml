<?xml version="1.0" encoding="UTF-8"?>

<package name="javax.swing">
	<Promise>@ThreadRoleImport("java.awt") for *</Promise>

	<Promise>@ThreadRoleTransparent for add*Listener(**) |
		remove*Listener(**)</Promise>

	<Promise>@ThreadRole(AWT) for (public **(**) in Abstract* | public
		**(**) in Default*) &amp; !(add*Listener(**) | remove*Listener(**))
	</Promise>
	<Promise>@ThreadRole(AWT) for get*(**) | set*(**) | new(**) |
		(add*(**) &amp; !(add*Listener(**)))</Promise>
	<Promise>@ThreadRole(AWT) for (is*(**) &amp;
		!(isEventDispatchThread())) | put*(**) | update*(**) | *register*(**)
	</Promise>
	<Promise>@ThreadRole(AWT) for show*(**) | scroll*(**) | *Path(**) |
		grab*(**)</Promise>

	<Promise>@ThreadRole(AWT) for paint*(**)</Promise>
	<Promise>@ThreadRole(AWT) for ensure*(**) | register*(**) |
		unregister*(**) | shared*(**)</Promise>
</package>
