<?xml version="1.0" encoding="UTF-8"?>

<package name="javax.swing.text">
	<class name="JTextComponent">
		<ThreadRoleImport>java.awt</ThreadRoleImport>
		
		<Promise>@ThreadRoleTransparent&quot; for add*Listener(**) |
			remove*Listener(**)</Promise>
			
		<Promise>@ThreadRole("GUI") for copy() | cut() | paste()</Promise>
		<Promise>@ThreadRole("GUI") for get*(**)</Promise>
		<Promise>@ThreadRole("GUI") for add*(**) &amp; !(add*Listener(**))
		</Promise>
		<Promise>@ThreadRole("GUI") for load*(**)</Promise>
		<Promise>@ThreadRole("GUI") for remove*(**) &amp;
			!(remove*Listener(**))</Promise>
		<Promise>@ThreadRole("GUI") for set*(**)</Promise>
		<Promise>@ThreadRole("GUI") for updateUI()</Promise>
	</class>
</package>
