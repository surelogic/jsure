<FindBugsFilter>
	<!-- match unused fields warnings in Messages classes in all packages -->
	<Match> 
	  <Class name="~.*\.Messages" />
	  <Bug code="MS" />
	</Match>
	<Match> 
	  <Class name="~edu\.cmu\.cs\.fluid\.mvc.*" />
	</Match>	
	<Match> 
	  <Class name="~edu\.cmu\.cs\.fluid\..*template" />
	</Match>			
	<Match> 
	  <Class name="~edu\.cmu\.cs\.fluid\.template\..*" />
	</Match>		
	<Match> 
	  <Class name="~edu\.cmu\.cs\.fluid\.java\.promise\..*" />
	</Match>		
	<Match> 
	  <Class name="~edu\.cmu\.cs\.fluid\.java\.operator\..*" />
	</Match>			
	<Match> 
	  <Class name="~edu\.cmu\.cs\.fluid\.parse\.ASCII.*Stream" />
	</Match>	
	<Match> 
	  <Class name="~edu\.cmu\.cs\.fluid\.java\.parse\.JavaParser.*" />
	</Match>		
	<Match> 
	  <Class name="~edu\.cmu\.cs\.fluid\.promise\.parse\.PromiseParser.*" />
	</Match>		
  <Match> 
	  <Class name="~edu\.cmu\.cs\.fluid\..*parse.JavaCharStream" />
	</Match>		
  <Match> 
	  <Class name="~edu\.cmu\.cs\.fluid\..*parse.ParseException" />
	</Match>
	<Match> 
	  <Class name="~com\.surelogic\.parse\..*Parser" />
	</Match>
</FindBugsFilter>