<?xml version="1.0" encoding="UTF-8"?>

<package name="java.util">
    <class name="CopyOnWriteArrayList">
        <ThreadSafe/>
        
        <constructor>
            <Borrowed>this</Borrowed>
            <RegionEffects>none</RegionEffects>
        </constructor>

        <constructor params="java.util.Collection">
            <Borrowed>this</Borrowed>
            <RegionEffects>arg0:Instance</RegionEffects>
        </constructor>

        <constructor params="java.lang.Object[]">
            <Borrowed>this</Borrowed>
            <RegionEffects>arg0:Instance</RegionEffects>
        </constructor>

        <method name="isEmpty">
            <Borrowed>this</Borrowed>
            <RegionEffects>reads this:Instance</RegionEffects>
        </method>

        <method name="iterator">
            <Borrowed allowReturn="true">this</Borrowed>
            <RegionEffects>writes this:Instance</RegionEffects>
            <Unique>return</Unique>
        </method>
        
        <method name="listIterator">
            <Borrowed allowReturn="true">this</Borrowed>
            <RegionEffects>writes this:Instance</RegionEffects>
            <Unique>return</Unique>
        </method>

        <method name="listIterator" params="int">
            <Borrowed allowReturn="true">this</Borrowed>
            <RegionEffects>writes this:Instance</RegionEffects>
            <Unique>return</Unique>
        </method>
        
        <class name="COWSubList">
            <constructor params="java.util.CopyOnWriteArrayList, int, int">
                <borrowed>this</borrowed>
                <RegionEffects>reads arg0:Instance</RegionEffects>
            </constructor>
            
            <method name="iterator">
                <Borrowed allowReturn="true">this</Borrowed>
                <RegionEffects>writes this:Instance</RegionEffects>
                <Unique>return</Unique>
            </method>

            <method name="listIterator">
                <Borrowed allowReturn="true">this</Borrowed>
                <RegionEffects>writes this:Instance</RegionEffects>
                <Unique>return</Unique>
                </method>

            <method name="listIterator" params="int">
                <Borrowed allowReturn="true">this</Borrowed>
                <RegionEffects>writes this:Instance</RegionEffects>
                <Unique>return</Unique>
            </method>
        </class>
    </class>
</package>
