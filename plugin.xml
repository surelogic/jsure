			<viewShortcut id="edu.cmu.cs.fluid.dcf.views.coe.ProblemsView">
			</viewShortcut>
			<viewShortcut id="edu.cmu.cs.fluid.dcf.views.coe.ProposedPromiseView">
			</viewShortcut>
		</perspectiveExtension>
	</extension>

	<extension point="org.eclipse.ui.perspectiveExtensions">
