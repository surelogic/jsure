		<run class="edu.cmu.cs.fluid.analysis.util.PromiseParser"
			category="required">
		</run>
		<prerequisite id="com.surelogic.jsure.client.eclipse.ConvertToIR">
		</prerequisite>
	</extension>
	<extension id="PromiseScrubber" name="Promise Scrubber"
